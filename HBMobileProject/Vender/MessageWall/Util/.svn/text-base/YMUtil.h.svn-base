#define IMPLEMENT_SINGLE(funcname) \
                                + (id)funcname \
                                { \
                                    static id i = nil; \
                                    static dispatch_once_t once; \
                                    dispatch_once(&once, ^{ \
                                    i = [[self alloc] init]; \
                                    }); \
                                    return i; \
                                }

