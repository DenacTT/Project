- (UILabel *)consultLabel
{
    if (nil == _consultLabel) {
        _consultLabel = [[UILabel alloc] init];
        _consultLabel.font = [UIFont systemFontOfSize:10];
//        _consultLabel.backgroundColor = [UIColor grayColor];
        _consultLabel.textColor = RGB(255, 255, 255);
        _consultLabel.textAlignment = NSTextAlignmentCenter;
//        [self addSubview:_consultLabel];
    }
    return _consultLabel;
}

- (UILabel *)introduceLabel
{
    if (nil == _introduceLabel) {
        _introduceLabel = [[UILabel alloc] init];
